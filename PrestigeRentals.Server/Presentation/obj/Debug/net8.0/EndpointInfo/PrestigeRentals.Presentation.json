{
  "openapi": "3.0.1",
  "info": {
    "title": "Prestige Rentals API",
    "description": "API for managing products in Prestige Rentals",
    "version": "v1"
  },
  "paths": {
    "/api/Vehicle/Vehicles": {
      "get": {
        "tags": [
          "Vehicle"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Vehicle/Vehicle": {
      "post": {
        "tags": [
          "Vehicle"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateVehicleDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateVehicleDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CreateVehicleDTO"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CreateVehicleDTO": {
        "type": "object",
        "properties": {
          "make": {
            "type": "string",
            "nullable": true
          },
          "model": {
            "type": "string",
            "nullable": true
          },
          "engineSize": {
            "type": "number",
            "format": "double"
          },
          "fuelType": {
            "type": "string",
            "nullable": true
          },
          "transmission": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}